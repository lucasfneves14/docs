{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "GraphorLM Docs",
  "colors": {
    "primary": "#6e2f84",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "appearance": {
    "strict": false
  },
  "favicon": "/favicon.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint Examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Product",
          "href": "https://app.graphorlm.com",
          "icon": "earth-americas"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.png",
    "dark": "/logo/dark.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:lucas@graphorlm.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Login",
      "href": "https://app.graphorlm.com/login"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/graphorlm",
      "github": "https://github.com/graphorlm",
      "linkedin": "https://linkedin.com/company/graphorlm"
    }
  }
}